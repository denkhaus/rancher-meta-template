
prefix = "/latest"
host = "http://rancher-metadata"
repeat = 66


[[set]]
name = "template 1"
template = "/etc/rancher-meta-template/nginx.tmpl"
dest = "/etc/nginx/sites-available/nginx.conf"

[set.check]
cmd = "nginx"
args = ["-t"]

[set.run]
cmd = "nginx"
args = ["-s", "reload"]


[[set]]
name = "template 2"
template = "/etc/rancher-meta-template/nginx1.tmpl"
dest = "/etc/nginx/sites-available/nginx1.conf"

[[set]]
name = "template 3"
template = "/etc/rancher-meta-template/nginx2.tmpl"
dest = "/etc/nginx/sites-available/nginx2.conf"
